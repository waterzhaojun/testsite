<section>
  <div>
    {{ section.settings.title }}
  </div>
  {%- for block in section.blocks -%}
    {{ block.name }}
  {% endfor %}
</section>

{% schema %}
  {
    "name": "Test Section",
    "settings": [
      {
        "id": "title",
        "type": "text",
        "label": "test section id",
        "default": "TtTTT"
      }
    ],
    "blocks": [
      {
        "name": "img",
        "type": "slide",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "test aaa section",
        "category": "Custom"
      }
    ]
  }
{% endschema %}